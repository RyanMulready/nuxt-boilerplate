{
    "name": "nuxt-boilerplate",
    "author": "Ryan Mulready",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "npm run type:check && nuxt build",
        "dev": "nuxt dev",
        "type:check": "vue-tsc",
        "lint:scss": "stylelint \"**/*.{scss,vue}\" --ignore-path .gitignore",
        "lint:js": "eslint --ext .ts,.js,.vue .",
        "lint:md": "markdownlint .",
        "prettier": "prettier --write \"**/*.{ts,js,mjs,vue,scss,json,md}\"",
        "lint": "npm run lint:js && npm run lint:scss && npm run lint:md && npm run prettier",
        "test": "vitest run --passWithNoTests",
        "postinstall": "nuxt prepare",
        "prepare": "husky"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.7.1",
        "@commitlint/config-conventional": "^19.7.1",
        "@intlify/eslint-plugin-vue-i18n": "^3.2.0",
        "@nuxt/test-utils": "^3.17.0",
        "@vue/test-utils": "^2.4.6",
        "eslint": "^9.21.0",
        "eslint-plugin-import": "^2.31.0",
        "happy-dom": "^17.1.8",
        "husky": "^9.1.7",
        "markdownlint-cli": "^0.44.0",
        "patch-package": "^8.0.0",
        "playwright-core": "^1.51.0",
        "postcss-html": "^1.8.0",
        "prettier": "^3.5.2",
        "sass": "^1.85.1",
        "stylelint": "^16.15.0",
        "stylelint-config-recommended": "^15.0.0",
        "stylelint-config-recommended-scss": "^14.1.0",
        "stylelint-config-recommended-vue": "^1.6.0",
        "stylelint-prettier": "^5.0.3",
        "stylelint-scss": "^6.11.1",
        "typescript": "^5.8.2",
        "vite": "^6.2.0",
        "vitest": "^3.0.7",
        "vue-tsc": "^2.1.6"
    },
    "dependencies": {
        "@nuxt/eslint": "^1.1.0",
        "@nuxt/fonts": "^0.10.3",
        "@nuxt/image": "^1.9.0",
        "@nuxtjs/i18n": "^9.2.1",
        "@pinia/nuxt": "^0.10.1",
        "@primeuix/themes": "^1.0.0",
        "@primevue/nuxt-module": "^4.3.1",
        "@vueuse/core": "^12.7.0",
        "@vueuse/nuxt": "^12.7.0",
        "nuxt": "^3.15.4",
        "pinia": "^3.0.1",
        "primeflex": "^4.0.0",
        "primeicons": "^7.0.0",
        "vue": "^3.5.13"
    }
}
